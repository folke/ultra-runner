{
  "name": "ultra-runner",
  "description": "Smart and beautiful script runner that hijacks any `npm run`, `yarn` and `npx` calls for ultra fast execution",
  "version": "3.6.0",
  "main": "lib/index.js",
  "repository": "http://github.com/folke/ultra-runner",
  "author": "Folke Lemaitre <folke.lemaitre@gmail.com>",
  "license": "MIT",
  "bin": {
    "ultra": "bin/ultra.js"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "files": [
    "lib",
    "bin",
    "*.md"
  ],
  "keywords": [
    "cli",
    "command-line",
    "concurrent",
    "concurrently",
    "execution",
    "fast",
    "npm",
    "parallel",
    "run",
    "runner",
    "scripts",
    "terminal",
    "yarn",
    "lerna",
    "pnpm",
    "monorepo",
    "build"
  ],
  "husky": {
    "hooks": {
      "prepare-commit-msg": "npx --no-install devmoji -e --lint"
    }
  },
  "scripts": {
    "preultra": "sleep 2",
    "postultra": "sleep 2",
    "ultra": "sleep 2 && sleep 2 && sleep 2",
    "true": "true",
    "profile": "0x -o --output-dir \"0x/{name}{pid}\"",
    "tn": "ts-node --transpile-only",
    "generate:emoji": "npx ts-node --transpile-only src/scripts/updater.ts",
    "generate:config": "npx ts-interface-builder src/config-options.ts",
    "prebuild": "yarn clean && yarn lint && yarn test",
    "build:ts": "npx tsc -p tsconfig.build.json",
    "build": "yarn build:ts",
    "clean": "npx rimraf lib coverage *.log build 0x",
    "test": "npx jest --runInBand",
    "test:cov": "npx jest --coverage --coverageProvider v8",
    "lint": "yarn lint:eslint && yarn lint:docs",
    "lint:eslint": "npx eslint bin/*.js src/*.ts __tests__/*.ts --cache",
    "lint:docs": "npx markdownlint README.md",
    "lint:fix": "yarn lint:eslint --fix",
    "prerelease": "yarn build",
    "release": "npx standard-version",
    "postrelease": "git push --follow-tags origin master && npm publish"
  },
  "ultra": {
    "concurrent": [
      "lint",
      "prebuild",
      "ultra"
    ]
  },
  "devDependencies": {
    "0x": "4.10.2",
    "@types/chai": "4.2.14",
    "@types/cross-spawn": "6.0.2",
    "@types/eslint": "7.2.6",
    "@types/eslint-plugin-prettier": "3.1.0",
    "@types/jest": "26.0.19",
    "@types/json5": "0.0.30",
    "@types/node": "13.13.36",
    "@types/prettier": "2.1.5",
    "@types/rimraf": "3.0.0",
    "@types/sinon": "9.0.9",
    "@types/sinon-chai": "3.2.5",
    "@types/standard-version": "7.0.0",
    "@types/wrap-ansi": "3.0.0",
    "@types/yamljs": "0.2.31",
    "@types/yargs": "15.0.11",
    "@typescript-eslint/eslint-plugin": "4.9.1",
    "@typescript-eslint/parser": "4.9.1",
    "builtin-modules": "3.1.0",
    "chai": "4.2.0",
    "conventional-changelog-cli": "2.1.1",
    "coveralls": "3.1.0",
    "devmoji": "2.1.13",
    "eslint": "7.15.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-chai-expect": "2.2.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.1.3",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.2.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-unicorn": "23.0.0",
    "husky": "4.3.5",
    "jest": "26.6.3",
    "markdownlint-cli": "0.25.0",
    "prettier": "2.2.1",
    "rimraf": "3.0.2",
    "sinon": "9.2.2",
    "sinon-chai": "3.5.0",
    "standard-version": "9.0.0",
    "ts-jest": "26.4.4",
    "ts-node": "9.1.1",
    "typescript": "4.1.3",
    "typesync": "0.8.0"
  },
  "dependencies": {
    "ansi-split": "^1.0.1",
    "chalk": "^4.0.0",
    "cross-spawn": "^7.0.1",
    "fast-glob": "^3.2.2",
    "globrex": "^0.1.2",
    "json5": "^2.1.1",
    "npm-run-path": "4.0.1",
    "pid-cwd": "^1.2.0",
    "ps-list": "^7.0.0",
    "shellwords-ts": "^3.0.0",
    "string-width": "^4.2.0",
    "tslib": "2.0.3",
    "type-fest": "^0.20.0",
    "wrap-ansi": "^7.0.0",
    "yamljs": "^0.3.0",
    "yargs": "^16.0.0"
  },
  "resolutions": {
    "minimist": "^1.2.2"
  }
}
